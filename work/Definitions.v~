`define ADDB  4'h07
`define ADDCA 4'h08
`define ADDCB 4'h09
`define SUBA  4'h0A
`define SUBB  4'h0B
`define SUBCA 4'h0C
`define SUBCB 4'h0D
`define ANDA  4'h0E
`define ANDB  4'h0F
`define ANDCA 4'h10
`define ANDCB 4'h11
`define ORA   4'h12
`define ORB   4'h13
`define ORCA  4'h14
`define ORCB  4'h15
`define ASLA  4'h16
`define ASRA  4'h17
`define JMP   4'h18
`define BAEQ  4'h19
`define BANE  4'h1A
`define BACS  4'h1B
`define BACC  4'h1C
`define BAMI  4'h1D
`define BAPL  4'h1E
`define BBEQ  4'h1F
`define BBNE  4'h20
`define BBCS  4'h21
`define BBCC  4'h22
`define BBMI  4'h23
`define BBPL  4'h24
`define NOP   4'h25


