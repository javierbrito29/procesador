`timescale 1ns / 1ps
`ifndef DEFINTIONS_V
`define DEFINTIONS_V
	
`default_nettype none	
`define LDA   4'h00
`define LDB   4'h01
`define LDCA  4'h02
`define LDCB  4'h03
`define STA   4'h04
`define STB   4'h05
